{
  "name": "@lunasec/cli",
  "version": "1.0.7",
  "description": "a wrapper around the LunaSec CLI enabling it to be used as an NPM package",
  "license": "MIT",
  "keywords": [
    "lunasec",
    "lunasec-cli",
    "tokenization",
    "tokenizer"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "compile": "yarn run tsc -p ./tsconfig.json",
    "watch": "yarn run compile -- -w",
    "types:generate": "yarn run json-schema-to-typescript"
  },
  "bin": {
    "lunasec": "./build/cli.js"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/find-config": "^1.0.1",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^14.0.0",
    "@types/source-map-support": "^0.5.4",
    "@types/yargs": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "genversion": "^3.0.1",
    "json-schema-to-typescript": "^10.1.5",
    "ts-node": "^10.3.0",
    "typescript": "~4.5.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.145.0",
    "@aws-cdk/aws-cloudfront": "1.145.0",
    "@aws-cdk/aws-cloudfront-origins": "1.145.0",
    "@aws-cdk/aws-dynamodb": "1.145.0",
    "@aws-cdk/aws-ecr": "1.145.0",
    "@aws-cdk/aws-events": "1.145.0",
    "@aws-cdk/aws-events-targets": "1.145.0",
    "@aws-cdk/aws-iam": "1.145.0",
    "@aws-cdk/aws-lambda": "1.145.0",
    "@aws-cdk/aws-s3": "1.145.0",
    "@aws-cdk/aws-s3-deployment": "1.145.0",
    "@aws-cdk/aws-secretsmanager": "1.145.0",
    "@aws-cdk/core": "1.145.0",
    "@aws-sdk/client-ecr": "3.39.0",
    "@aws-sdk/client-s3": "3.39.0",
    "@aws-sdk/client-sts": "3.39.0",
    "@lunasec/secure-frame-front-end": "^1.0.7",
    "adm-zip": "^0.5.9",
    "aws-cdk": "1.145.0",
    "aws-cdk-local": "1.65.8",
    "aws-sdk": "^1.0.0",
    "cmd-ts": "^0.7.0",
    "decompress": "^4.2.1",
    "find-config": "^1.0.0",
    "import-local": "^3.0.3",
    "js-yaml": "^4.1.0",
    "progress": "^2.0.3",
    "source-map-support": "^0.5.20",
    "yargs": "~17.5.1"
  }
}
