{
  "name": "@lunasec/cli",
  "version": "1.0.0",
  "description": "lunasec CLI utility",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "bin": {
    "lunasec": "./bin/run"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "lunasec",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "prerun": "./src/hooks/prerun"
    }
  },
  "scripts": {
    "clean": "rm -rf build",
    "compile": "tsc -p tsconfig.json",
    "build:main": "tsc -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "types": "build/main/index.d.ts",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@lunasec/node-sdk": "^1.0.0",
    "dotenv": "^9.0.2",
    "oclif": "^1.17.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "typescript": "^4.2.0"
  }
}
