{
  "name": "docs",
  "version": "1.0.7",
  "private": true,
  "license": "CC-BY-SA-4.0",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn run openapi:generate && TYPEDOC_WATCH=true docusaurus start --port 4000",
    "build": "yarn run openapi:generate && TYPEDOC_WATCH=false yarn run build:docs && yarn insert:analytics",
    "build:docs": "docusaurus build",
    "insert:analytics": "find ./build -iname \"*.html\" | xargs sed -i -e \"/<\\/body>/r ../tools/analytics-inserter/analytics-fragment.txt\" -e \"s/<\\/body>//\"",
    "swizzle": "docusaurus swizzle",
    "deploy": "yarn run openapi:generate &&  docusaurus deploy",
    "deploy:s3-redirect-config": "s3-redirect-generator --inputConfig ./redirect-config.js --bucket ",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "openapi:generate": "redoc-cli bundle ../lunadefend/api-spec/schema/full-tokenizer.yaml --output static/tokenizer-api-spec-static/index.html"
  },
  "dependencies": {
    "@docusaurus/core": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@docusaurus/theme-search-algolia": "2.3.1",
    "@lunasec/s3-redirect-generator": "^1.0.7",
    "@mdx-js/react": "^1.6.21",
    "@openapitools/openapi-generator-cli": "^2.3.9",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "jsonp": "~0.2.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-mailchimp-form": "^1.0.2",
    "redoc-cli": "^0.12.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "docusaurus-plugin-typedoc": "0.15.3",
    "typedoc": "0.21.5",
    "typedoc-plugin-markdown": "3.10.4",
    "typescript": "~4.5.5"
  }
}
