{
  "name": "frontend-redux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "@rtk-query/graphql-request-base-query": "^1.0.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "5.1.3",
    "bootstrap-switch-button-react": "^1.2.0",
    "graphql-request": "^3.7.0",
    "history": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "prettify-date": "^1.4.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-helmet-async": "^1.2.2",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "redux-first-history": "^5.0.8",
    "sass-loader": "^12.4.0",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-rtk-query": "^2.2.2",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.20.42",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.22",
    "@types/testing-library__react": "^10.2.0",
    "apollo": "2.33.9",
    "npm-run-all": "^4.1.5",
    "sass": "^1.49.0"
  },
  "scripts": {
    "sass:build": "sass src/assets/scss/main:public/css",
    "dev:sass:watch": "sass --watch src/assets/scss/main:public/css",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:build sass:autoprefixer",
    "dev:react-scripts": "DISABLE_ESLINT_PLUGIN=true PORT=3000 react-scripts start",
    "start": "yarn run dev:sass:watch & yarn run dev:react-scripts",
    "build": "yarn run generate && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "schema:download": "apollo client:download-schema --endpoint http://localhost:8080/v1/graphql schema-codegen.graphql",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
