{
  "name": "lunatrace-backend",
  "license": "BSL-LunaTrace",
  "type": "commonjs",
  "main": "build/server.js",
  "types": "build/server.d.ts",
  "scripts": {
    "start": "concurrently 'yarn dev:graphql:watch' 'yarn run start:server'",
    "start:server": "nodemon ./src/server.ts --enable-source-maps",
    "start:worker": "nodemon src/workers/queue/start.ts --enable-source-maps",
    "start:job:update-vulns": "NODE_OPTIONS=\\\"--max-old-space-size=8192\\\" yarn run ts-node --files src/workers/triggered-jobs/upsert-vulnerabilities.ts",
    "builds:rescan": "yarn run ts-node ./src/workers/triggered-jobs/rescan-latest-builds.ts",
    "prod:worker": "node build/workers/handler.js --enable-source-maps",
    "test:scan": "yarn run ts-node --files ./src/tests/local-scan-test.ts",
    "test:scan:upload": "yarn run ts-node --files ./src/tests/upload-scan.ts",
    "test:load": "yarn run ts-node --files ./src/tests/local-load-vulnerabilities-test.ts",
    "test:jest": "jest ./src/tests/npm-audit.test.ts",
    "guides:update": "yarn run ts-node --files src/guide-ingester/cmd.ts ../../../guides",
    "test:deleted": "yarn run ts-node --files ./src/tests/deleted-vulns-scan.ts",
    "start:prod": "pm2-runtime start ./build/server.js --name demo-back-end",
    "generate:hasura-calls": "graphql-codegen --config codegen.hasura.yml",
    "generate:custom-calls": "graphql-codegen --config codegen.lunatrace-custom.yml",
    "generate:github-calls": "graphql-codegen --config codegen.github.yml",
    "generate": "yarn run generate:github-calls && yarn run generate:hasura-calls && yarn run generate:custom-calls",
    "dev:graphql:watch": "yarn run generate:hasura-calls && watch 'yarn run generate:hasura-calls' ./src/hasura-api/graphql",
    "compile": "tsc -p tsconfig.json && cp src/graphql-yoga/schema.graphql build/graphql-yoga/schema.graphql",
    "compile:watch": "tsc -p tsconfig.json -w"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.53.0",
    "@aws-sdk/client-s3": "^3.46.0",
    "@aws-sdk/client-sqs": "^3.53.0",
