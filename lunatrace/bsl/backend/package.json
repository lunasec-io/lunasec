    "@graphql-tools/load": "~7.5.9",
    "@graphql-tools/schema": "~8.3.9",
    "@graphql-yoga/node": "~2.3.0",
    "@lunatrace/logger": "workspace:~",
    "@lunatrace/lunatrace-common": "workspace:~",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/graphql": "^4.8.0",
