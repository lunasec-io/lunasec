{
  "name": "lunatrace-backend",
  "license": "BSL-LunaTrace",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"nodemon build/server.js\"",
    "start:sqs-listener": "tsc && concurrently \"tsc -w\" \"nodemon build/sqs/read-queue.js\"",
    "test:scan": "yarn run ts-node --files ./src/tests/local-scan-test.ts",
    "test:scan:upload": "yarn run ts-node --files ./src/tests/upload-scan.ts",
    "test:load": "yarn run ts-node --files ./src/tests/local-load-vulnerabilities-test.ts",
    "test:jest": "jest",
    "vulnerabilities:update": "yarn run ts-node --files ./src/tests/upsert-vulns.ts",
    "test:deleted": "yarn run ts-node --files ./src/tests/deleted-vulns-scan.ts",
    "start:prod": "pm2-runtime start ./build/server.js --name demo-back-end",
    "generate:schema": "gq http://localhost:8080/v1/graphql -H \"X-Hasura-Admin-Secret: myadminsecretkey\" --introspect > schema.graphql",
    "generate:hasura-calls": "yarn run generate:schema && graphql-codegen --config codegen.yml",
    "graphqurl": "^1.0.1",
    "compile": "tsc -p tsconfig.json",
    "compile:watch": "tsc -p tsconfig.json -w"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.53.0",
    "@aws-sdk/client-s3": "^3.46.0",
    "@aws-sdk/client-sqs": "^3.53.0",
    "@aws-sdk/credential-provider-node": "^3.46.0",
    "@aws-sdk/hash-node": "^3.46.0",
    "@aws-sdk/lib-storage": "^3.53.1",
    "@aws-sdk/protocol-http": "^3.46.0",
    "@aws-sdk/s3-request-presigner": "^3.46.0",
    "@aws-sdk/types": "^3.46.0",
    "@aws-sdk/url-parser": "^3.46.0",
    "@aws-sdk/util-format-url": "^3.46.0",
    "@octokit/auth-app": "^3.6.1",
    "@octokit/graphql": "^4.8.0",
    "@ory/kratos-client": "v0.8.2-alpha.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1082.0",
    "axios": "^0.26.0",
    "concurrently": "^7.0.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "5.0.0-beta.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "graphql-tag": "^2.12.6",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "pg-promise": "^10.11.1",
    "pm2": "^5.1.2",
    "semver": "^7.3.5",
    "slugify": "^1.6.5",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.4.0",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@types/aws-lambda": "^8.10.89",
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/semver": "^7.3.9",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.23.2",
    "generate-lockfile": "^0.0.11",
    "graphqurl": "^1.0.1",
    "jest": "^27.5.1",
    "source-map-support": "^0.5.16",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "~4.5.5"
  }
}
